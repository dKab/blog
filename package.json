    "clean-css": "^4.2.3",
    "date-fns": "^2.14.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-toc-done-right": "^4.1.0",
    "terser": "^4.8.0"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
